{
  "name": "dailyworth-client",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-api\"",
    "watch-client": "node webpack/dev-server.js",
    "start-dev": "NODE_ENV=development PORT=3000 node ./bin/server",
    "devtools": "DEV_TOOLS=1 npm start",
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "prod": "concurrent --kill-others \"npm run start-prod\" \"npm run start-api\"",
    "start-prod": "NODE_ENV=production PORT=3000 node ./bin/server",
    "start-api": "API_PORT=8080 node ./bin/apiServer",
    "lint": "eslint src",
    "test": "NODE_ENV=test mocha --compilers js:babel/register --require ./test/helper.js --recursive",
    "test:watch": "npm run test -- --watch"
  },
